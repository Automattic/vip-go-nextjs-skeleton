{
  "name": "wpnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev": "npm run prebuild && npm run start:server",
    "dev:debug": "npm run prebuild && npm run start:server:debug",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prebuild": "npm run codegen",
    "prebuild:watch": "npm run codegen -- --watch",
    "start": "NODE_ENV=production npm run start:server",
    "start:server": "node server/index.js",
    "start:server:debug": "NODE_OPTIONS='--inspect' node server/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@types/ioredis": "^4.26.5",
    "@types/react": "^17.0.11",
    "cross-fetch": "^3.1.4",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "ioredis": "^4.27.6",
    "next": "^11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1"
  }
}
