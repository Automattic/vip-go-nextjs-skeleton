{
  "name": "vip-go-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "npm run codegen -- --watch",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "prebuild": "npm run codegen",
    "predev": "npm run codegen:watch &",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "vip:preflight": "npx @automattic/vip-go-preflight-checks"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-operations": "2.3.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.8",
    "@types/ioredis": "4.28.8",
    "@types/react": "17.0.39",
    "cross-fetch": "3.1.5",
    "dotenv": "16.0.0",
    "graphql": "16.3.0",
    "ioredis": "4.28.5",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-editorconfig": "3.2.0",
    "eslint-plugin-jest": "26.1.1",
    "jest": "27.5.1",
    "ts-jest": "27.1.3"
  }
}
