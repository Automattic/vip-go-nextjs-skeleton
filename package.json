{
  "name": "vip-go-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "npm run codegen -- --watch",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "prebuild": "npm run codegen",
    "predev": "npm run codegen:watch &",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "vip:preflight": "npx @automattic/vip-go-preflight-checks"
  },
  "dependencies": {
    "@apollo/client": "3.8.6",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/fragment-matcher": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "cross-fetch": "4.0.0",
    "dotenv": "16.3.1",
    "graphql": "16.8.1",
    "ioredis": "5.3.2",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.6",
    "@types/react": "18.2.31",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-plugin-editorconfig": "4.0.3",
    "eslint-plugin-jest": "27.4.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
