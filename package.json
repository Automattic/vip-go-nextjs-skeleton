{
  "name": "vip-go-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev": "npm run prebuild && npm run start:server",
    "dev:debug": "npm run prebuild && npm run start:server:debug",
    "build": "next build",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "prebuild": "npm run codegen",
    "prebuild:watch": "npm run codegen -- --watch",
    "start": "NODE_ENV=production npm run start:server",
    "start:server": "node server/index.js",
    "start:server:debug": "NODE_OPTIONS='--inspect' node server/index.js",
    "test": "jest",
    "vip:preflight": "npx @automattic/vip-go-preflight-checks"
  },
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@types/ioredis": "^4.28.7",
    "@types/react": "^17.0.38",
    "cross-fetch": "^3.1.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "http-proxy-middleware": "^2.0.2",
    "ioredis": "^4.28.3",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.0.9",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3"
  }
}
