{
  "name": "vip-go-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "dev": "npm run prebuild && npm run start:server",
    "dev:debug": "npm run prebuild && npm run start:server:debug",
    "build": "next build",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "prebuild": "npm run codegen",
    "prebuild:watch": "npm run codegen -- --watch",
    "start": "NODE_ENV=production npm run start:server",
    "start:server": "node server/index.js",
    "start:server:debug": "NODE_OPTIONS='--inspect' node server/index.js",
    "test": "jest",
    "vip:preflight": "npx @automattic/vip-go-preflight-checks"
  },
  "dependencies": {
    "@apollo/client": "^3.4.8",
    "@graphql-codegen/cli": "^2.1.0",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/typescript": "^2.1.0",
    "@graphql-codegen/typescript-operations": "^2.1.0",
    "@graphql-codegen/typescript-react-apollo": "^3.1.0",
    "@types/ioredis": "^4.26.7",
    "@types/react": "^17.0.19",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "http-proxy-middleware": "^2.0.1",
    "ioredis": "^4.27.8",
    "next": "^11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5"
  }
}
