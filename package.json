{
  "name": "vip-go-nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "npm run codegen -- --watch",
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "lint": "next lint --ignore-path .gitignore",
    "lint:fix": "next lint --fix --ignore-path .gitignore",
    "prebuild": "npm run codegen",
    "predev": "npm run codegen:watch &",
    "start": "next start",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "vip:preflight": "npx @automattic/vip-go-preflight-checks"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/fragment-matcher": "3.3.1",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.4",
    "@types/react": "17.0.50",
    "cross-fetch": "3.1.5",
    "dotenv": "16.0.3",
    "graphql": "16.6.0",
    "ioredis": "5.2.3",
    "next": "12.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-shadow": "^20.0.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@types/jest": "29.1.2",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-editorconfig": "4.0.2",
    "eslint-plugin-jest": "27.1.2",
    "jest": "29.2.0",
    "jest-environment-jsdom": "29.2.0",
    "ts-jest": "29.0.3"
  }
}
